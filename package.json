{
  "name": "mern",
  "version": "1.0.0",
  "description": "this application will allow you to search, delete and save book from a google books API.",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.2",
    "nodemon": "^1.19.4"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "mongodb": "^3.5.7",
    "mongoose": "^5.3.16",
    "morgan": "^1.10.0",
    "node.js": "0.0.1-security"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jomain12/GoogleBooksReact.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/Jomain12/GoogleBooksReact/issues"
  },
  "homepage": "https://github.com/Jomain12/GoogleBooksReact#readme"
}
